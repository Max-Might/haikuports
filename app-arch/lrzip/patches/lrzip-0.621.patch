From e133af3943ffbdfe1b58dc819a87e09dfd9c90ad Mon Sep 17 00:00:00 2001
From: Kostadin Damyanov <maxmight@gmail.com>
Date: Thu, 8 Oct 2015 18:44:29 +0300
Subject: [PATCH 1/1] applying patch lrzip-0.621.patch

---
 configure.ac | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/configure.ac b/configure.ac
index 7bfe011..f1838bc 100644
--- a/configure.ac
+++ b/configure.ac
@@ -53,7 +53,7 @@ AC_SUBST(SHELL)
 AC_SYS_LARGEFILE
 AC_FUNC_ALLOCA
 AC_PROG_CC_C99
-AS_IF([test "x$ac_cv_prog_cc_c99" = "xno"], 
+AS_IF([test "x$ac_cv_prog_cc_c99" = "xno"],
 	AC_MSG_ERROR([C compiler does not support C99], 1))
 AC_CHECK_PROG([HAVE_POD2MAN], [pod2man], [yes])
 AS_IF([test "$HAVE_POD2MAN" != "yes"],
@@ -111,9 +111,9 @@ AC_C_INLINE
 
 AC_C_BIGENDIAN
 
-AC_CHECK_LIB(pthread, pthread_create, ,
+AC_CHECK_LIB(root, pthread_create, ,
         AC_MSG_ERROR([Could not find pthread library - please install libpthread]))
-AC_CHECK_LIB(m, sqrt, ,
+AC_CHECK_LIB(root, sqrt, ,
         AC_MSG_ERROR([Could not find math library - please install libm]))
 AC_CHECK_LIB(z, compress2, ,
 	AC_MSG_ERROR([Could not find zlib library - please install zlib-dev]))
